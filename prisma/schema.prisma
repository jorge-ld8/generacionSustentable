generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("LOCALDB_URL")
}

model actionA3 {
   id Int @id @default(autoincrement())
   nombre String
   descripcion String
   fecha_inicio Date 
   fecha_final Date
   localidad String
   nro_participantes Int
   nro_mujeres Int
   nro_pob_ind Int
   nro_pob Int
   nro_pob_lgbtiq Int
   nro_talleres Int
   nro_materiales_didacticos Int
}

