generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model actionA1 {
  id                Int      @id @default(autoincrement())
  nombre            String
  type              String
  fecha_inicio      DateTime
  fecha_final       DateTime
  localidad         String
  nro_participantes Int
  nro_mujeres       Int
  nro_nobin         Int
  nro_noid          Int
  nro_pob_ind       Int?
  nro_pob_lgbtiq    Int
  nro_pob_rural     Int?
  nro_pob_16_29     Int?
  organizacion      String
  tipo_localidad    String
  descripcion       String
  nombre_real       String
  imgUrl            String?
}

model User {
  id           Int    @id @default(autoincrement())
  username     String @unique
  password     String
  organizacion String
  nombre       String
  apellido     String
}
